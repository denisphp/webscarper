{
    "name": "webscarper",
    "description": "Web Scarper",
    "type": "project",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mobidev-php/yii2-swagger.git"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.6",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-jui" : "2.0.*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-redis": "~2.0.0",
        "mobidev/yii2-swagger": "dev-master",
        "guzzlehttp/guzzle": "5.*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset":{
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            }
        }
    },
    "scripts": {
        "pre-dependencies-solving": "composer global require \"fxp/composer-asset-plugin:^1.3.1\""
    }
}
